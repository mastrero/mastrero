{
	"name": "mastrero",
	"description": "Repository for my portfolio. Built with Gatsby",
	"private": true,
	"version": "0.1.0",
	"license": "0BSD",
	"keywords": [
		"mastrero",
		"ArunKumar Nadikattu",
		"gatsby"
	],
	"scripts": {
		"develop": "gatsby develop",
		"start": "set GATSBY_ENV=development && npm run develop",
		"build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
		"serve": "gatsby serve",
		"clean": "gatsby clean",
		"format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
		"lint": "cd src && eslint --ext .jsx,.js Components/**/* Styles/** UI/**"
	},
	"dependencies": {
		"babel-plugin-styled-components": "^1.11.1",
		"emailjs-com": "^2.6.3",
		"gatsby": "^2.24.61",
		"gatsby-image": "^2.4.19",
		"gatsby-plugin-brotli": "^2.0.0",
		"gatsby-plugin-google-analytics": "^2.3.14",
		"gatsby-plugin-manifest": "^2.4.30",
		"gatsby-plugin-netlify-cache": "^1.2.0",
		"gatsby-plugin-offline": "^3.2.28",
		"gatsby-plugin-react-helmet": "^3.3.11",
		"gatsby-plugin-sharp": "^2.6.36",
		"gatsby-plugin-styled-components": "^3.3.11",
		"gatsby-source-filesystem": "^2.3.30",
		"gatsby-transformer-sharp": "^2.5.16",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^6.1.0",
		"styled-components": "^5.2.0"
	},
	"devDependencies": {
		"@hot-loader/react-dom": "^16.13.0",
		"prettier": "2.1.1"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mastrero/mastrero"
	},
	"bugs": {
		"url": "https://github.com/mastrero/mastrero/issues"
	}
}
